<?xml version="1.0" encoding="UTF-8"?>
<openerp>
    <data>
       
	<record id="npg_hr_timesheet_line_date_search" model="ir.ui.view">
           <field name="name">npg.hr.analytic.timesheet.date.search</field>
           <field name="model">hr.analytic.timesheet</field>
           <field name="inherit_id" ref="hr_timesheet.hr_timesheet_line_search"/>
           <field name="arch" type="xml">
                <field name="account_id" position='after'>
                    <field name="search_from" filter_domain="[('date','&gt;=',self)]"/>
                    <field name="search_to" filter_domain="[('date','&lt;=',self)]"/>
                </field>
           </field>
   	
   	</record> 
       
    </data>
</openerp>
